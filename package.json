{
  "name": "widap-web",
  "license": "0BSD",
  "dependencies": {
    "leaflet": "^1.4.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-select": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "github:widap/widap-web"
  },
  "scripts": {
    "build:map": "browserify js/power-plant-map.js -o bundles/power-plant-map-bundle.js",
    "build:dashboard": "browserify js/unit-level-dashboard.js -t babelify -o bundles/unit-level-dashboard-bundle.js",
    "build-prod:map": "browserify js/power-plant-map.js -t babelify -p tinyify -o bundles/power-plant-map-bundle.js",
    "build-prod:dashboard": "browserify js/unit-level-dashboard.js -t babelify -p tinyify -o bundles/unit-level-dashboard-bundle.js",
    "watch": "watchify js/unit-level-dashboard.js -t babelify -o bundles/unit-level-dashboard-bundle.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-minify": "^0.5.0",
    "babel-watch": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "tinyify": "^2.5.0"
  }
}
